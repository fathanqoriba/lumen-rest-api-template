{
    "name": "cbxtechcorp/lumen-api-boilerplate-jwt",
    "description": "Lumen Framework Rest API Boilerplate using JWT AUTH",
    "keywords": ["framework", "laravel", "lumen", "microservice", "JWT", "REST", "API", "boilerplate", "api boilerplate"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "laravel/lumen-framework": "^6.0",
        "illuminate/mail": "^6.0",
        "illuminate/redis": "^6.0",
        "tymon/jwt-auth": "^1",
        "dingo/api": "^2.4",
        "flipbox/lumen-generator": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "cbxtechcorp/lumen-api-routes-list": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan jwt:secret"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "liyu": {
            "type": "path",
            "url": "/Users/liyu/Code/dingo-serializer-switch"
        }
    }
}
